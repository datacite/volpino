<div class="row">
  <div class='col-md-9'>
    <div class="tab-content">
      <div class="tab-pane active panel-list" id="login">
        <%= render :partial => 'users/login' %>
      </div>
      <div class="tab-pane" id="account">
        <%= render :partial => 'users/account' %>
      </div>
      <% if @member.present? %>
        <div class="tab-pane" id="member">
          <%= render :partial => 'members/show' %>
        </div>
      <% end %>
      <div class="tab-pane panel-list" id="orcid">
        <%= render :partial => 'users/orcid' %>
      </div>
      <div class="tab-pane panel-list" id="impactstory">
        <%= render :partial => 'users/impactstory' %>
      </div>
    </div>
  </div>
  <div class='col-md-3'>
    <ul class="nav nav-tabs nav-stacked">
      <li class="active"><a href="#login" data-toggle="tab">Social Login</a></li>
      <li><a href="#account" data-toggle="tab">Your Account</a></li>
      <% if @member.present? %>
        <li><a href="#member" data-toggle="tab">Member Record</a></li>
      <% end %>
      <li><a href="#orcid" data-toggle="tab">ORCID Claims</a></li>
      <li><a href="#impactstory" data-toggle="tab">ImpactStory</a></li>
    </ul>
  </div>
</div>
