<% if @flash.present? && @flash[:omniauth].blank? %>
  <% @flash.each do |name, msg| %>
    <% if msg.present? and msg.is_a?(String) %>
      <div class="text-center flash-message alert alert-<%= name == "notice" ? "info" : "warning" %>">
        <%= msg %>
      </div>
    <% end %>
  <% end %>
<% end %>
